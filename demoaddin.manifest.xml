        <bt:Image id="icon32" DefaultValue="https://dsmsgeu-demoaddin.azurewebsites.net/Images/icon32.png"/>
        <bt:Image id="icon80" DefaultValue="https://dsmsgeu-demoaddin.azurewebsites.net/Images/icon80.png"/>
